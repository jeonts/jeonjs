  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@types/acorn": "^6.0.4",
    "@types/node": "^24.9.1",
    "@woby/chk": "0.1.13",
    "acorn": "^8.15.0",
    "autoprefixer": "^10.4.21",
    "prismjs": "^1.30.0",
    "tailwindcss": "^4.1.16",
