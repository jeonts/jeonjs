{
  "name": "jeon",
  "version": "1.0.1",
  "description": "JEON (JSON-based Executable Object Notation) converter for TypeScript/JavaScript",
  "main": "dist/jeon-converter.cjs.js",
  "module": "dist/jeon-converter.es.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build:lib": "vite build --mode lib",
    "build:app": "vite build",
